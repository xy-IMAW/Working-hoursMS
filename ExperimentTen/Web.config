<configuration>
  <connectionStrings>
    <add name="ConnStr" connectionString="Data Source=LENOVO-PC\SQLEXPRESS;Initial Catalog=Working_hoursMS;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <!--
